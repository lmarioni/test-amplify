    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/.env",
            "program": "${file}",
            "env": {
                "WORKSPACE": "${workspaceFolder}/amplify/backend/function/sharedLayer",
                "NODE_MODULES": "${workspaceFolder}/amplify/backend/function/sharedLayer/lib/nodejs/node_modules/"
            }
        },
    ]
}